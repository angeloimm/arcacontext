<!-- CODICE DOMANDA -->
<div class="form-group col-md-4">
	<label for="codiceHeader" class="control-label">Cod. dom.</label> <input
		type="text" class="form-control" id="codiceHeader"  readonly>
</div>
<!-- CODICE FISCALE -->
<div class="form-group col-md-8">
	<label for="cfHeader" class="control-label">Codice fiscale
		(*)</label> <input type="text" class="form-control" id="cfHeader"
		placeholder="Codice fiscale" readonly>
</div>
<!-- NOMINATIVO -->
<div class="form-group col-md-12">
	<label for="nome" class="control-label">Nominativo (*)</label> <input
		type="text" class="form-control" id="nomeHeader" placeholder="Nome" readonly>
</div>
<!-- LUOGO DI NASCITA -->
<div class="form-group col-md-5">
	<label for="luogoHeader" class="control-label">Nato a (*)</label> <input
		type="text" class="form-control" id="luogoHeader"
		placeholder="Luogo di nascita" readonly>
</div>
<!-- PROVINCIA -->
<div class="form-group col-md-2">
	<label for="provinciaHeader" class="control-label">Prov. (*)</label> <input
		type="text" class="form-control" id="provinciaHeader" readonly>
</div>
<!-- DATA -->
<div class="form-group col-md-3">
	<label for="data_header" class="control-label">il (*) </label>
	<div class="input-group date">
		<input id="data_header" name="data"
			class="form-control" readonly="readonly" type="text" value="" /> <span
			class="input-group-addon"> <span
			class="glyphicon glyphicon-calendar"></span>
		</span>
	</div>
</div>
<!-- SESSO -->
<div class="form-group col-md-2">
	<label for="sessoHeader" class="control-label">Sesso (*)</label> <input
		type="text" class="form-control selectpicker" id="sessoHeader" readonly>
</div>